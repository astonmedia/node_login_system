<%- include('header') -%>
<div id="login" class="center-div">
  <div class="container w-25 border py-5">
    <div class="title text-center pb-5">
      <h2 class="font-weight-bold">Login System</h2>
      <span class="span-title">Log in for the existing user</span>
    </div>
    <form action="" method="post">
      <div class="form-group">
        <input
          type="email"
          class="form-control"
          name="email"
          placeholder="email"
        />
        <small class="form-text text-muted text-left"
          >Register Email address</small
        >
      </div>
      <div class="form-group">
        <input
          type="password"
          name="password"
          class="form-control"
          placeholder="Enter your password"
        />
      </div>
      <button type="submit" class="btn btn-success rounded-pill mt-2">
        Submit
      </button>
    </form>
  </div>
</div>
<%- include('footer') -%>
